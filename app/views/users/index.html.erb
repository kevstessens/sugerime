<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.birthdate %></td>
    <td><%= link_to 'Nueva oferta', offers_new_directed_offer_path(user) %></td>
