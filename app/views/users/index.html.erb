<div class="row">
  <div class="span8">
<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Name</th>
    <th>Last name</th>
    <th>Birthdate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.birthdate %></td>
    <td><%= link_to 'Nueva oferta', offers_directed_offer_path(:user_id => user.id) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %> </br></br>
<%= link_to "Salir", destroy_user_session_path, :method => :delete %>
  </div>
</div>
